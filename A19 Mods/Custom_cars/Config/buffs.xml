<configs>

	<append xpath="/buffs/buff[@name='buffStatusCheck01']/effect_group">
		<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="VehicleChecker">
			<requirement name="NotHasBuff" buff="VehicleChecker"/>
		</triggered_effect>
	</append>
	<append xpath="/buffs">

		<buff name="VehicleChecker" hidden="true">
			<stack_type value="replace"/>
			<duration value="500"/>
			<update_rate value=".1"/>
			<effect_group>
			
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="selfAOE" range="100" buff="4x4TruckChecker">
					<requirement name="EntityTagCompare" target="other" tags="jeep"/>
					<requirement name="NotHasBuff" target="other" buff="4x4TruckChecker"/>
				</triggered_effect>
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="selfAOE" range="100" buff="HellBeastBikeCheck">
					<requirement name="EntityTagCompare" target="other" tags="hellbeast"/>
					<requirement name="NotHasBuff" target="other" buff="HellBeastBikeCheck"/>
				</triggered_effect>
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="selfAOE" range="100" buff="ArmyTruckCheck">
					<requirement name="EntityTagCompare" target="other" tags="armytruck"/>
					<requirement name="NotHasBuff" target="other" buff="ArmyTruckCheck"/>
				</triggered_effect>
				
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="selfAOE" range="100" buff="ArmoredCarCheck">
					<requirement name="EntityTagCompare" target="other" tags="armoredcar"/>
					<requirement name="NotHasBuff" target="other" buff="ArmoredCarCheck"/>
				</triggered_effect>				
			</effect_group>
		</buff>


        <buff name="HellBeastBikeCheck" hidden="true">	
			<stack_type value="ignore"/>			
			<effect_group>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="true"  transform_path="headlight"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="p_torch_wall" local_offset=".15,.1,-.2" local_rotation="0,0,0" parent_transform="headlight"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="particleeffects/p_torch_wall" local_offset="-.15,.1,-.2" local_rotation="0,0,0" parent_transform="headlight"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false"  transform_path="headlight"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/OutdoorDecor/cowSkullPrefab" local_offset="0,.1,.5" local_rotation="45,0,0" parent_transform="Origin/handlebar_joint"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/LootContainers?weapon_bags_smallPrefab.prefab" local_offset="0,1.4,-0.8" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Other/Items?Weapons/Melee/Spear/spear_steel_Prefab.prefab" local_offset=".24,.58,0.9" local_rotation="90,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="#Other/Items?Weapons/Melee/Spear/spear_steel_Prefab.prefab" local_offset="-.24,.58,0.9" local_rotation="90,0,0" parent_transform="Origin"/>
			</effect_group>
		</buff>


		
		<buff name="ArmoredBuff" tooltip_key="Protected" icon="ui_game_symbol_defense">
			<stack_type value="replace"/>
			<update_rate value=".1"/>
			<duration value="2"/>
			<effect_group>
				<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffInjuryBleeding"/>
				<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffInjuryStunned01"/>
				<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="ParasiteAttack"/>
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="ArmoredAttackCheck"/>
				<triggered_effect trigger="onSelfBuffRemove" action="RemoveBuff" buff="ArmoredAttackCheck"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemoveBuff" buff="ArmoredAttackCheck"/>
				<passive_effect name="ElementalDamageResist" operation="base_add" value="200" tags="cold,heat,electrical"/>
				<passive_effect name="PhysicalDamageResist" operation="base_add" value="200"/>
				<passive_effect name="PhysicalDamageResist" operation="base_add" value="200" tags="coredamageresist"/>
			</effect_group>
		</buff>
		<buff name="ArmoredAttackCheck" hidden="true">
			<stack_type value="ignore"/>
			<update_rate value=".5"/>
			<effect_group>
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" target="selfAOE" range="1.2" buff="ArmoredAttack">
					<requirement name="EntityTagCompare" target="other" tags="zombie,enemy"/>
				</triggered_effect>
			</effect_group>
		</buff>
		<buff name="ArmoredAttack" hidden="true">
			<stack_type value="ignore"/>
			<damage_type value="bloodloss"/>
			<duration value="1"/>
			<effect_group>
				<triggered_effect trigger="onSelfBuffStart"  target="self" action="ModifyStats" stat="Health" operation="subtract" value="20"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="p_blood_impact" local_offset="0,0,0"  parent_transform="Spine"/>
				<triggered_effect trigger="onSelfBuffStart" action="PlaySound" sound="woodhitorganic"/>
				<triggered_effect trigger="onSelfBuffFinish" action="RemoveParticleEffectFromEntity" particle="p_blood_impact" />
			</effect_group>
		</buff>
		<buff name="ArmoredCarCheck" hidden="true">	
			<stack_type value="ignore"/>			
			<effect_group>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="Box1"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="Box2"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="BoxR"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="BoxPipeL"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="BoxPipeR"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false"  transform_path="car_mesh"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/LootContainers?dumpsterPrefab.prefab" local_offset="-.49,.75,0" local_rotation="90,0,90" parent_transform="Physics"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="LOD1"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="LOD0"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Physics" transform_path="shadow_caster"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/LootContainers?dumpsterPrefab.prefab" local_offset=".49,.75,0" local_rotation="90,0,-90" parent_transform="BoxL"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="BoxL" transform_path="LOD1"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="BoxL" transform_path="LOD0"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="BoxL" transform_path="shadow_caster"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="#Entities/LootContainers?dumpsterPrefab.prefab" local_offset=".49,.75,0" local_rotation="90,0,-90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/LootContainers?dumpsterPrefab.prefab" local_offset="-.49,.75,0" local_rotation="90,0,90" parent_transform="Origin"/>

				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="#Entities/Vehicles?Car/car_tirePrefab.prefab" local_offset="2.2,.4,0" local_rotation="90,0,-90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/Vehicles?Car/car_tirePrefab.prefab" local_offset="-2.2,.4,0" local_rotation="90,0,90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/Vehicles?Car/car_wheel_uprightPrefab.prefab" local_offset="-2.2,.4,-1" local_rotation="90,0,90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="#Entities/Vehicles?Car/car_wheel_uprightPrefab.prefab" local_offset="2.2,.4,-1" local_rotation="90,0,-90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="#Entities/Vehicles?Car/car_wheelPrefab.prefab" local_offset="-2.2,.4,1" local_rotation="90,0,90" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachParticleEffectToEntity" particle="#Entities/Vehicles?Car/car_wheelPrefab.prefab" local_offset="2.2,.4,1" local_rotation="90,0,-90" parent_transform="Origin"/>

				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Electrical/generatorPrefab" local_offset=".6,2.45,0" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Doors/steel_hatch_v3Prefab" local_offset="-.5,2.7,0" local_rotation="0,0,0" parent_transform="Origin"/>


				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Industrial/barrelOilPrefab" local_offset="-0.2,1.5,-1.3" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Industrial/barrelGasPrefab" local_offset="0.6,1.5,-1.3" local_rotation="0,0,0" parent_transform="Origin"/>

				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Electrical/spotlightPrefab" local_offset="0.6,1,1.7" local_rotation="0,0,0" parent_transform="GameObject"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="GameObject" transform_path="Rotate_LOD0"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="GameObject" transform_path="Rotate_LOD1"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="GameObject" transform_path="Rotate_LOD2"/>

				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Lighting/player_spotlightPrefab" local_offset="-0.6,1,1.7" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="turret_cone"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="camera"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="Spotlight"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="MainLight"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="Rotate_LOD0"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="Rotate_LOD1"/>
				<triggered_effect trigger="onSelfBuffStart" action="SetTransformActive" active="false" parent_transform="Origin" transform_path="Rotate_LOD2"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Traps/trap_spikes01_damage03Prefab" local_offset="-.75,.5,1.7" local_rotation="90,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Traps/trap_spikes01_damage02Prefab" local_offset=".25,.5,1.7" local_rotation="-90,180,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Traps/trap_spikes01_damage04Prefab" local_offset="-.25,.5,1.7" local_rotation="90,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Traps/trap_spikes01_damage05Prefab" local_offset=".75,.5,1.7" local_rotation="90,0,0" parent_transform="Origin"/>

				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Electrical/motion_sensor" local_offset=".93,1.8,1.85" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AttachPrefabToEntity" prefab="Entities/Electrical/loudspeakerPrefab" local_offset="-.93,1.8,1.8" local_rotation="0,0,0" parent_transform="Origin"/>
				<triggered_effect trigger="onSelfBuffStart" action="AddBuff" target="selfAOE" range=".1" buff="ArmoredBuff">
					<requirement name="EntityTagCompare" target="other" tags="player"/>
				</triggered_effect>


			</effect_group>
		</buff>
		
		
	</append>

</configs>